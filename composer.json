{
  "name": "berlioz/http-client",
  "description": "Berlioz HTTP Client",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://www.berlioz-framework.com",
  "authors": [
    {
      "name": "Ronan Giron",
      "email": "ronan.giron@berlioz-framework.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Berlioz\\Http\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Berlioz\\Http\\Client\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-zlib": "*",
    "berlioz/http-message": "^1.0.1",
    "psr/http-client": "^1.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0",
    "symfony/process": "^4.1"
  },
  "suggest": {
    "berlioz/html-selector": "@dev"
  },
  "provide": {
    "psr/http-client-implementation": "^1.0",
    "psr/log-implementation": "^1.0"
  }
}
